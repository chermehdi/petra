<div class="col-md-10 col-sm-8 col-xs-8 main-content">
    <div class="row">
        <div class="col-md-8 col-md-offset-2">
            <div class="card">
                <div class="row">
                    <div class="col-md-4 user-image">
                        <img src="images/<%= image %>" alt="user image" class="rounded">
                    </div>
                    <div class="col-md-8 user-info">
                        <div class="user-name">
                            <h3> <%= [student.firstName, student.lastName].join(' ') %></h3>
                        </div>
                        <div class="user-cne">
                            <p><b> CNE : </b><%= student.cne %></p>
                        </div>
                        <div class="user-email">
                            <p><b> Email : </b> <%= student.email %></p>
                        </div>
                        <div class="user-phone">
                            <p><b> Numero de telephone : </b> <%= student.phone %></p>
                        </div>
                    </div>
                </div>
                <div class="row block">
                    <div class="col-md-6 col-md-offset-1 controlls no-padd block">
                        <a href="/admin/list" class="btn btn-primary pull-right no-marg"> <i class="fa fa-arrow-left"> </i>&nbsp;
                            &nbsp;
                            go Back</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>